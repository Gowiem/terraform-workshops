terraform {
  backend "s3" {}
}

provider "aws" {
  version = "~> 2.0"
}

resource "aws_key_pair" "my_key_pair_01" {
  key_name   = "rockholla-di-${var.student_alias}-01"
  public_key = "ssh-rsa  ${var.student_alias}+di@rockholla.org"
}

resource "aws_key_pair" "my_key_pair_02" {
  key_name   = "rockholla-di-${var.student_alias}-02"
  public_key = "ssh-rsa  ${var.student_alias}+di@rockholla.org"
}
